%import common.ESCAPED_STRING
%import common.SIGNED_NUMBER
%import common.WS
%ignore WS

?start: line

?line : DOLLAR_SIGN value

DOLLAR_SIGN : "$"

value : seal | strict

seal : "seal" number

strict : qualification string

qualification : "at least" | "at most" | about |

atleast : "at least"
atmost : "at least"
about : "at least"


string : ESCAPED_STRING
number : SIGNED_NUMBER



